{
  "schemaVersion": 1,
  "id": "voidfog",
  "version": "${version}",
  "name": "Void Fog",
  "description": "Brings back the old Void Fog.",
  "authors": [ "Tamaized", "Sollace" ],
  "contact": {
    "sources": "https://github.com/Sollace/Void-Fog",
    "issues": "https://github.com/Sollace/Void-Fog/issues"
  },
  "license": "MIT",
  "icon": "voidfog_logo.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.tamaized.voidfog.VoidFog"
    ],
    "modmenu": [
      "com.tamaized.voidfog.modmenu.VFModMenuFactory"
    ]
  },
  "mixins": [
    "voidfog.mixin.json"
  ],
  "depends": {
    "minecraft": "${minecraftVersion}",
    "fabricloader": "${loaderVersion}",
    "java": ">=17",
    "fabric-api": "${fabricVersion}",
    "kirin": "*"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/Sollace/Void-Fog/releases",
        "modmenu.modrinth": "https://modrinth.com/mod/void-fog"
      },
      "update_checker": true
    }
  }
}